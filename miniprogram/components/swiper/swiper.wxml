<!--components/swiper/swiper.wxml-->
<view class="swiper-sec">
        <view class="page-section page-section-spacing swiper">
            <swiper easing-function="linear" display-multiple-items="{{1}}" indicator-dots="{{swiperInfo.indicatorDots}}" autoplay="{{swiperInfo.autoplay}}" interval="{{swiperInfo.interval}}" circular="{{swiperInfo.circular}}" duration="{{swiperInfo.duration}}">
                <block wx:for="{{swiperArr}}" wx:key="img">
                    <swiper-item>
                        <view class="swiper-item"> <image  src="{{item.img}}" alt="" /></view>
                    </swiper-item>
                </block>
            </swiper>
        </view>
    </view>

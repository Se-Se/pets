/* components/canvas/canvas.wxss */
.container{
    width: 100%;
    height: 100%;
    position: relative;
}
.canvas-content{
    width: 100%;
    height: 100%;
    position: relative;
    /* background: #198a8a; */
}
.canvas-content .canvas{
    height: 600rpx;
    width: 500rpx;
    /* background-color: thistle; */
    position: relative;
    z-index: -1;
    left: 50%;
    top: 50%;
    transform: translate(-50%,-50%);
}
